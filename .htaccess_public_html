<FilesMatch "\.(html|css|js|json|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)$">
    Order allow,deny
    Allow from all
</FilesMatch>

# Configuraci칩n de seguridad - bloquear acceso directo a archivos Python
<FilesMatch "\.(py|pyc|pyo|pyd|log|db|sqlite)$">
    Order allow,deny
    Deny from all
</FilesMatch>

